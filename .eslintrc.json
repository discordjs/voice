{
	"root": true,
	"extends": "marine/prettier/node",
	"parserOptions": {
		"extraFileExtensions": [".mjs"],
		"project": "./tsconfig.eslint.json"
	},
	"rules": {
		"@typescript-eslint/naming-convention": [
			"error",
			{
				"selector": "enumMember",
				"format": ["PascalCase"]
			}
		],
		"no-redeclare": "off"
	}
}
